{
    "patients": {
        "description": "Query the clinical data at the patient level",
        "query_params": {
            "centre": {
                "table": "patient",
                "column": "test_centre",
                "description": "Search by specifying the acronym of the test centre",
                "example": "CMN"
            },
            "patient": {
                "table": "patient",
                "column": "centre_patient_no",
                "description": "Search by specifying the sequence number of the patient (not the Id)",
                "example": "1"
            },
            "patient_trial_id": {
                "table": "patient",
                "column": "patient_trial_id",
                "description": "Search by specifying the trial ID of the patient",
                "example": "1501001"
            },
            "tumour_site": {
                "table": "patient",
                "column": "tumour_site",
                "description": "Search by affected organ or anatomical region",
                "example": "prostate"
            },
            "trial": {
                "table": "patient",
                "column": "clinical_trial",
                "description": "Search by clinical trial",
                "example": "SPARK"
            }
        },
        "object_fields": [
            {
                "property" : "patient_trial_id",
                "description" : "The TROG provided patient ID",
                "field" : {
                    "table": "patient", 
                    "column": "patient_trial_id"
                },
                "type": "str"
            },
            {
                "property" : "test_centre",
                "description" : "Acronym of the test/treatment centre, where the RT fraction was carried out",
                "field" : {
                    "table": "patient", 
                    "column": "test_centre"
                },
                "type": "str"
            },
            {
                "property" : "patient_no", 
                "description" : "The sequence number of the patient within a test centre",
                "field" : {
                    "table": "patient", 
                    "column": "centre_patient_no"
                },
                "type": "int"
            }, 
            {
                "property" : "age", 
                "description" : "Age of the patient at the time of the treatment",
                "field" : {
                    "table": "patient", 
                    "column": "age"
                },
                "type": "int"
            },
            {
                "property" : "gender", 
                "description" : "Administrative sex of the patient",
                "field" : {
                    "table": "patient", 
                    "column": "gender"
                },
                "type": "str"
            },
            {
                "property" : "tumour_site", 
                "description" : "The organ/region of the tumour being treated",
                "field" : {
                    "table": "patient", 
                    "column": "tumour_site"
                },
                "type": "str"
            },
            {
                "property" : "num_markers", 
                "description" : "The number of markers implanted in the treatment region",
                "field" : {
                    "table": "patient", 
                    "column": "number_of_markers"
                },
                "type": "int"
            },
            {
                "property" : "trial", 
                "description" : "Name of the clinical trial",
                "field" : {
                    "table": "patient", 
                    "column": "clinical_trial"
                },
                "type": "int"
            }                
        ],
        "db_relations": [
            {
                "table": "patient"
            }
        ]             
    },
